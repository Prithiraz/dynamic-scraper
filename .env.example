# Environment configuration for Dynamic Flight Scraper
# IMPORTANT: This application ONLY uses real flight data sources
# Configure your API keys below to access real flight data

# Amadeus API Configuration (for real flight data)
AMADEUS_API_KEY=your_amadeus_api_key_here
AMADEUS_API_SECRET=your_amadeus_api_secret_here

# RapidAPI Configuration (for Skyscanner and other APIs)
RAPIDAPI_KEY=your_rapidapi_key_here

# CRITICAL SETTING: Never change this to True
# This ensures no fake data is ever generated
ALLOW_FAKE_DATA=False